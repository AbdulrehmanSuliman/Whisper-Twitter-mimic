
{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "mado7abib007/swfront",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links":["backend"],
            "memory":10
        },
        {
            "name": "backend",
            "image": "mado7abib007/swbackend",
            "hostname": "client",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 8000,
                    "containerPort":8000
                }
            ],

            "memory":10
        }
       
    ]
}