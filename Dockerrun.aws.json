
{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "frontend-docker",
            "image": "${{ secrets.DOCKERHUB_USERNAME }}/swfront",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort":80
                }
            ],
            "links":["backend"],
            "memory":10
        },
        {
            "name": "backend-docker",
            "image": "${{ secrets.DOCKERHUB_USERNAME }}/swbackend",
            "hostname": "client",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 8080,
                    "containerPort":8080
                }
            ],

            "memory":10
        }
       
    ]
}